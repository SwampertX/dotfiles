{
    "coc.preferences.formatOnSaveFiletypes": [
        "typescriptreact",
        "typescript",
        "javascript",
        "javascriptreact",
        "css",
        "markdown",
        "java"
    ],
    "languageserver": {
        "golang": {
            "command": "gopls",
            "args": [],
            "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
            "filetypes": ["go"]
        },
        "ccls": {
            "command": "ccls",
            "filetypes": ["c", "cpp", "objc", "objcpp"],
            "rootPatterns": [".ccls", "compile_commands.json", ".vim/", ".git/", ".hg/"],
            "initializationOptions": {
                "cache": {
                    "directory": "/tmp/ccls"
                }
            }
        }
    },
    "efm": {
        "command": "efm-langserver",
        "args": [],
        "filetypes": ["vim", "eruby", "markdown"]
    },
    "solargraph": {
        "commandPath": "/home/titanx/.rbenv/shims/solargraph",
        "formatting": true,
        "diagnostics": true
    },
    "java": {
        "format": {
            "settings.url": "/home/titanx/.config/coc/extensions/node_modules/redhat.java/eclipse-formatter.xml",
            "settings.profile": "Google",
            "comments.enabled": false
        }
    },
    "python.jediEnabled": false
}
